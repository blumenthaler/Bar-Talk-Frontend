n\"\n        },\n        body: JSON.stringify(data)\n      })\n      .then(resp => resp.json())\n      .then(obj => {\n          if (obj.error) {\n            console.log(obj.error)\n          }\n          else {\n            dispatch(addNewComment(obj))\n            // getAllComments will not work for this\n            // Redux store is updated, but CommentsContainer will not re-render\n            dispatch(getAllComments())\n            // dispatch(getAllCocktails())\n            history.push(`${match.url}/`)\n          }\n      })\n      .catch(console.log)\n  }\n}\n\n\nexport const deleteComment = comment => {\n  return dispatch => {\n  dispatch({type: \"DELETING_COMMENT\"})\n    return fetch(`${DOMAIN_URL}/api/v1/comments/` + comment.id, {\n        credentials: \"include\",\n        method: \"DELETE\",\n    })\n    .then(resp => resp.json())\n    .then(r => {\n      if (r.error) {\n        console.log(r.error)\n      }\n      else {\n        dispatch(deletedComment(comment))\n        dispatch(getAllComments())\n      }\n    })\n    .catch(console.log)\n  }\n}","/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/users/Logout.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/domain.js",[],["222","223"],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/reducers/comments.js",["224"],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/Navbar.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/NavBar.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/containers/AllCocktailsContainer.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/containers/ProfileContainer.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/recipes/input/RecipeInput.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/recipes/input/RecipeWithCocktail.js",[],"/Users/alexdblument