ter(comment => comment.id !== action.comment.id)\n            return {\n                ...state,\n                comments: {\n                    ...state.comments,\n                    data: [filteredComments]\n                },\n                loading: false\n            }\n         \n        default:\n            return state;\n    }\n}","/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/Navbar.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/NavBar.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/containers/AllCocktailsContainer.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/containers/ProfileContainer.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/recipes/input/RecipeInput.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/recipes/input/RecipeWithCocktail.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/recipes/input/NewRecipeButton.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/comments/CommentInput.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/comments/NewCommentButton.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/recipes/edit/EditRecipeForm.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/recipes/edit/EditRecipeButton.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/recipes/edit/CancelEditButton.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/recipes/input/CancelInputButton.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/comments/CancelCommentButton.js",[],{"ruleId":"229","replacedBy":"230"},{"ruleId":"231","replacedBy":"232"},{"ruleId":"233","severity":1,"message":"234","line":1,"column":8,"nodeType":"235","mes