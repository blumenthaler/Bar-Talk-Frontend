iners/RecipesContainer.js",["227"],"import React from 'react';\nimport currentUser from '../../reducers/currentUser.js';\nimport { connect } from 'react-redux';\nimport Recipes from '../recipes/Recipes.js';\nimport RecipeInput from '../recipes/input/RecipeInput.js';\nimport { addRecipe, editingRecipe } from '../../actions/recipes.js';\nimport { NewRecipeButton } from '../recipes/input/NewRecipeButton.js';\n\n\nclass RecipesContainer extends React.Component {\n\n    constructor(props) {\n        super(props)\n        this.state = {\n            isEmptyState: true,\n            showingRecipeForm: false\n        }\n    }\n\n    triggerRecipeForm = () => {\n        this.setState(prevState => {\n            return {\n                showingRecipeForm: !prevState.showingRecipeForm,\n                isEmptyState: !prevState.isEmptyState\n            }\n        })\n    }\n\n    render() {\n        if (!this.props.cocktail) {\n            return (\n                  <RecipeInput currentUser={this.props.currentUser} addRecipe={this.props.addRecipe} cocktail={this.props.cocktail} history={this.props.history} match={this.props.match} triggerRecipeForm={this.triggerRecipeForm} /> \n            )\n        }\n        else {\n        const filteredRecipes = this.props.recipes.filter(recipe => recipe.relationships.user.data.id === this.props.user.id)\n        return (\n            <div>\n                <Recipes user={this.props.user} recipes={filteredRecipes} currentUser={this.props.currentUser} comments={this.props.comments} editingRecipe={this.props.editingRecipe} />\n\n                {this.state.showingRecipeForm ? <RecipeInput currentUser={this.props.currentUser} addRecipe={this.props.addRecipe} cocktail={this.props.cocktail} history={this.props.history} match={this.props.match} triggerRecipeForm={this.triggerRecipeForm} /> : \n                <NewRecipeButton currentUser={this.props.currentUser} addRecipe={this.props.addRecipe} cocktail={this.props.cocktail} history={this.props.history} triggerRecipeForm={this.triggerRecipeForm}/>}\n            </div>\n        )\n        }\n    }\n}\n\n\nexport default connect(null, {addRecipe, editingRecipe})(RecipesContainer)","/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/actions/users.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/containers/UsersContainer.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/users/Users.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/users/User.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/containers/CommentsContainer.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/comments/Comments.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/comments/Comment.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/actions/comments.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/components/users/Logout.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/domain.js",[],"/Users/alexdblumenthaler/bar-talk-react/bar-talk-frontend/src/reducers/comments.js",["228"],"export default (state = {\n    comments: [],\n    loading: false\n}, action) => {\n    switch (action.type) {\n        case \"LOADING_COMMENTS\":\n            return {\n                ...state,\n                loading: true\n            }\n        case \"ADD_COMMENTS\":\n            return {\n                ...state,\n                comments: action.comments,\n                loading: false\n            }\n        case \"ADDING_NEW_COMMENT\":\n            return {\n                ...state,\n                loading: true\n            }\n        case \"ADD_NEW_COMMENT\":\n            \n            const thisUser = action.comment.included.find(data => data.type === 'user')\n            const thisRecipe = action.comment.included.find(data => data.type === 'recipe